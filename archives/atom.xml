---
layout: default
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>James Diao's Repository</title>
	<link href="https://jamesdiao.github.io/archives/atom.xml" rel="self"/>
	<link href="https://jamesdiao.github.io/archives"/>
	<updated>{{ site.time | archives }}</updated>
	<id>https://jamesdiao.github.io/archives</id>
	<author>
		<name>James Diao</name>
		<email>diao.james@gmail.com</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="https://jamesdiao.github.io{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>https://jamesdiao.github.io{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
